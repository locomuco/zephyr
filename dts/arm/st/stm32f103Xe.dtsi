#include <st/stm32f103Xb.dtsi>

/ {
	soc {
		spi3: spi@40003C00 {
			compatible = "st,stm32-spi";
			#address-cells = <1>;
			#size-cells = <0>;
			reg = <0x40003C00 0x400>;
			interrupts = <51 5>;
			status = "disabled";
		};

		pinctrl: pin-controller {
			reg = <0x40010800 0x2000>;

			gpiof: gpio@40011c00 {
				compatible = "st,stm32-gpio";
				gpio-controller;
				#gpio-cells = <2>;
				reg = <0x40011c00 0x400>;
				clocks = <&rcc STM32_CLOCK_BUS_APB2 0x00000081>;
				label = "GPIOF";
			};
		};
	};
};
